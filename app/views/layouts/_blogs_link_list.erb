<div class="left_menu_widget_element_title">
PARTNERS
</div>
<table width='100%' border=0>
<% for pl in @left_column_partner_links %>
   <tr valign='top' align='center'>
     <td class='partner_link_list'>
       <div align=center>
       <% if pl.publish_image %>
          <%= link_to( image_tag(pl.image, :border => 0, :width => 130, :alt => pl.title ), pl.tag ) %>
       <% else %>
          <%= link_to pl.title, pl.tag %><br/>
       <% end %>
        </div>
     </td>
   </tr>
<% end %>
</table>