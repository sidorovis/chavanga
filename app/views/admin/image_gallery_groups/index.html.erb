<h1>Listing image_gallery_groups</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Parent group</th>
    <th>Visible</th>
  </tr>

<% @image_gallery_groups.each do |image_gallery_group| %>
  <tr>
    <td><%=h image_gallery_group.title %></td>
    <td><%=h image_gallery_group.description %></td>
    <td><%=h image_gallery_group.parent_group.title %></td>
    <td><%=h image_gallery_group.visible %></td>
    <td><%= link_to 'Show', image_gallery_group %></td>
    <td><%= link_to 'Edit', edit_admin_image_gallery_group_path(image_gallery_group) %></td>
    <td><%= link_to 'Destroy', admin_image_gallery_group_url(image_gallery_group), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New image_gallery_group', new_admin_image_gallery_group_path %>